{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "scripts": {
    "serverFront": "webpack-dev-server --inline --progress --port 8080",
    "build-prod": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
